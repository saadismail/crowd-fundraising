--> Create a new Schema
CREATE SCHEMA `Proj` ;

--> User Table DDL
CREATE TABLE `Proj`.`User` (
  `Email` VARCHAR(30) NOT NULL,
  `funds_to_Proj` DOUBLE NULL,
  `Name` VARCHAR(30) NOT NULL,
  `fundsSent` DOUBLE NULL,
  `CNIC` BIGINT(13) NOT NULL,
  `creditCardNumber` BIGINT(16) NOT NULL,
  `votedFor` VARCHAR(30) NULL,
  `UserId` INT NOT NULL AUTO_INCREMENT,
  UNIQUE INDEX `Email_UNIQUE` (`Email` ASC) VISIBLE,
  PRIMARY KEY (`CNIC`, `creditCardNumber`),
  UNIQUE INDEX `CNIC_UNIQUE` (`CNIC` ASC) VISIBLE,
  UNIQUE INDEX `creditCardNumber_UNIQUE` (`creditCardNumber` ASC) VISIBLE,
  UNIQUE INDEX `UserId_UNIQUE` (`UserId` ASC) VISIBLE);

--> Project Table DDL
CREATE TABLE `Proj`.`Projects` (
  `Project_Id` INT NOT NULL AUTO_INCREMENT,
  `fundsRecieved` DOUBLE NULL DEFAULT 0,
  `Milestone` DOUBLE NOT NULL DEFAULT 0,
  `Status` VARCHAR(10) NOT NULL,
  `Title` VARCHAR(45) NOT NULL,
  `Category` VARCHAR(45) NOT NULL,
  `MaxVotes` INT NOT NULL,
  `Description` VARCHAR(45) NOT NULL,
  `Votes` INT NOT NULL DEFAULT 0,
  PRIMARY KEY (`Project_Id`),
  UNIQUE INDEX `Project_Id_UNIQUE` (`Project_Id` ASC) VISIBLE,
  UNIQUE INDEX `Title_UNIQUE` (`Title` ASC) VISIBLE,
  UNIQUE INDEX `Category_UNIQUE` (`Category` ASC) VISIBLE);

-->Administrators Table DDL
CREATE TABLE `Proj`.`Administrators` (
  `AdminId` INT NOT NULL,
  `Post` VARCHAR(45) NOT NULL,
  `Name` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`AdminId`),
  UNIQUE INDEX `AdminId_UNIQUE` (`AdminId` ASC) VISIBLE);

--> Categories Table DDL
CREATE TABLE `Proj`.`Categories` (
  `Name` VARCHAR(45) NOT NULL,
  `Projects` VARCHAR(45) NULL);

